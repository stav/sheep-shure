{
  "$schema": "https://raw.githubusercontent.com/nickkuk/nickkuk.github.io/master/tauri-v2-capabilities-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "fs:default",
    "store:default",
    "shell:allow-open"
  ]
}
